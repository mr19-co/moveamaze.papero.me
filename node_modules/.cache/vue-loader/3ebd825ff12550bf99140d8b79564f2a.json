"file":"/home/max/WebstormProjects/moveamaze.papero.me/src/components/PreGamePhase.vue","sourceRoot":"","sourcesContent":["<template>\n  <div class=\"big-div-pre-game-phase\">\n    {{ JSON.stringify(props.lobby) }}\n    <div class=\"players\">\n      <div\n          class=\"player-and-name-container\"\n          v-for=\"id in Object.keys(state.lobby.players)\" :key=\"id\"\n      >\n        <div\n            class=\"player\"\n            :class=\"'player-' + colors[state.lobby.players[id].color]\"\n            :id=\"id == state.lobby.yourLocalId ? 'player-you' : ''\"\n        />\n        <div class=\"name\">\n          {{ state.lobby.players[id].username }}\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { reactive } from \"vue\";\n\nexport default {\n  name: \"PreGamePhase\",\n  props: {\n    lobby: { required: true }\n  },\n  setup(props) {\n    const state = reactive({\n      lobby: props.lobby\n    })\n\n    const colors = {\n      0: 'blue',\n      1: 'green',\n      2: 'red',\n      3: 'yellow'\n    }\n\n    return {\n      state,\n      colors\n    }\n  }\n}\n</script>\n\n<style scoped>\n  .player-red {\n    background-color: red;\n  }\n\n  .player-yellow {\n    background-color: yellow;\n  }\n\n  .player-blue {\n    background-color: blue;\n  }\n\n  .player-green {\n    background-color: green;\n  }\n\n  #player-you{\n    font-weight: bold;\n  }\n\n  .player {\n    width: 2rem;\n    height: 2rem;\n    margin: .1rem;\n  }\n\n  .player-and-name-container{\n    display: flex;\n    flex-direction: row;\n  }\n\n  .big-div-pre-game-phase {\n    position: relative;\n  }\n\n  .players {\n    position: absolute;\n    left: 50%;\n    transform: translateX(-50%);\n  }\n\n  .name{\n    align-self: center;\n  }\n</style>"]}]}